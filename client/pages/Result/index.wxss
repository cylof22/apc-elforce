page {
    background: #F6F6F6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

.preview-container {
  width: 100%;
  height: 500rpx;
}

.toolbar {
  height: 60rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.btn-share {
  font-size: 35rpx;
  width: 150rpx;
  height: 70rpx;
  padding-top: 0rpx;
  border-radius: 6px!important;
  background: #0000F6;
}