page {
    background: #F6F6F6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

.preview-container {
  width: 100%;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.uploader-image {
  width: 100%;
  height: 1005rpx;
}

.toolbar {
  height: 60rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  margin-top: 8rpx;
}

